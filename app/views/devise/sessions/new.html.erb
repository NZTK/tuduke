  <div class= "body" >
    <div class ="container">
      <div class ="row">
        <div class = "col-lg-10 col-lg-offset-1 col-md-10 col-md-offset-1 col-xs-12 " >
          <div class ="devise_container">
            <h2>Log in</h2>

            <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
              <%= render "devise/shared/error_messages", resource: resource %>

              <div class ="field devise_field">
                <%= f.text_field :username, autofocus: true, autocomplete: "username" , placeholder: "\uF304 ユーザーネーム" ,class: "fa" %>
              </div>
              <div class="field devise_field">
                <%= f.password_field :password, autocomplete: "current-password", placeholder: "\uF023 パスワード", class: "fa" %>
              </div>
              <%= link_to "パスワードをお忘れですか?", new_password_path(resource_name) %><br />
              <% if devise_mapping.rememberable? %>
                <div class="field">
                  <%= f.check_box :remember_me %>
                  <%= f.label :remember_me %>
                </div>
              <% end %>
              <div class="actions">
                <%= f.submit "Log in" %>
              </div>
            <% end %>
              <div class ="action_btn">
                <%= button_to "Sign up", new_user_registration_path ,method: :get%>
              </div>
          </div>
        </div>
      </div>
    </div>
</div>

<div  style="text-align: center;">
<a href="#" onClick="history.back(); return false;">前のページにもどる</a>
</div>